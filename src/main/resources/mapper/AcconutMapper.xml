<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xry.mapper.AccountMapper">
  <insert id="save">
      insert  into account(name,money) values (#{name},#{money})
  </insert>
    <select id="selectacc" resultType="com.xry.bean.Account">
        select * from account
    </select>
</mapper>